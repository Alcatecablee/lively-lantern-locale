
{
  "name": "@neurolint/cli",
  "version": "1.0.0",
  "description": "NeuroLint CLI - Multi-layer automated React/Next.js code fixing system",
  "main": "index.js",
  "bin": {
    "neurolint": "./bin/neurolint.js"
  },
  "scripts": {
    "start": "node bin/neurolint.js",
    "build": "node build.js",
    "start:server": "node server.js",
    "install": "chmod +x bin/neurolint.js",
    "fix-all": "node bin/neurolint.js fix",
    "fix-dry-run": "node bin/neurolint.js fix --dry-run",
    "fix-verbose": "node bin/neurolint.js fix --verbose",
    "fix-layer-1": "node bin/neurolint.js layer-1",
    "fix-layer-2": "node bin/neurolint.js layer-2", 
    "fix-layer-3": "node bin/neurolint.js layer-3",
    "fix-layer-4": "node bin/neurolint.js layer-4",
    "fix-layer-5": "node bin/neurolint.js layer-5",
    "fix-layer-6": "node bin/neurolint.js layer-6",
    "analyze": "node bin/neurolint.js analyze",
    "test": "node bin/neurolint.js test",
    "clean": "npx rimraf release backups .neurolint-temp"
  },
  "keywords": [
    "react",
    "nextjs", 
    "typescript",
    "code-fixing",
    "automation",
    "cli",
    "neurolint"
  ],
  "author": "NeuroLint Team",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.50.0",
    "axios": "^1.0.0",
    "commander": "^11.0.0",
    "express": "^4.18.0",
    "cors": "^2.8.5",
    "@babel/parser": "^7.0.0",
    "chalk": "^4.1.2",
    "glob": "^10.3.0"
  },
  "devDependencies": {
    "rimraf": "^6.0.1"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/neurolint/cli.git"
  },
  "bugs": {
    "url": "https://github.com/neurolint/cli/issues"
  },
  "homepage": "https://neurolint.dev"
}
